[Unit]
Description=Select creating access point or joining wifi based on switch
Requisite=multi-user.target
After=sys-subsystem-net-devices-wlan0.device network-pre.target
Before=network.target
Wants=network.target

[Service]
Type=oneshot
ExecStart=/usr/share/wifi-ap-sw/startup

[Install]
WantedBy=multi-user.target
WantedBy=sys-subsystem-net-devices-wlan0.device
